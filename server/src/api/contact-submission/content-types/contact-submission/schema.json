{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "queryRelated": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "General Inquiry",
        "Support",
        "Partnership"
      ]
    },
    "message": {
      "type": "text",
      "required": true
    },
    "submittedAt": {
      "type": "date"
    }
  }
}
